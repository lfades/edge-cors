{
  "name": "edge-cors",
  "version": "0.2.1",
  "description": "CORS for edge workers, works with Next.js Middleware",
  "type": "module",
  "main": "dist/cors.js",
  "module": "dist/cors.js",
  "types": "dist/cors.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier --write .",
    "test": "tape test/**/*.js --experimental-specifier-resolution=node",
