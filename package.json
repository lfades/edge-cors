    "dev": "npm run build -- --watch"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "author": "Luis Alvarez (@lfades)",
  "license": "MIT",
  "devDependencies": {
    "node-fetch": "^3.0.0",
    "prettier": "^2.4.1",
    "tape": "^5.3.1",
    "typescript": "^4.4.4"
  }
}
